script/source = "extends Control


onready var Banner : Button = $VBoxContainer/Banner
onready var Native : Button = $VBoxContainer/Native
onready var Interstitial : Button = $VBoxContainer/Interstitial
onready var Rewarded : Button = $VBoxContainer/Rewarded

func _ready():
	AdMob.init()
	#AdMob.load_banner()
	AdMob.connect(\"banner_loaded\", self, \"_on_AdMob_banner_loaded\")
	AdMob.connect(\"banner_destroyed\", self, \"_on_AdMob_banner_destroyed\")
	
func _on_AdMob_banner_loaded():
	Banner.disabled = false
	
func _on_AdMob_banner_destroyed():
	Banner.disabled = true
	AdMob.load_banner(AdMob.ad_unit_id.banner.Android)

func _on_Banner_pressed():
	AdMob.destroy_banner()"

